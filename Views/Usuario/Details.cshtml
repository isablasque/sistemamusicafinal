@model sistemamusicafinal.Models.Usuario

@{
    ViewData["Title"] = "Details";
}

<h1>Detalhes do usuário</h1>

<div>
    <hr />
    <dl class="detalhes col-md-4">
        <dt class = "col-sm-10">
            @Html.DisplayNameFor(model => model.NomeUsuario)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.NomeUsuario)
        </dd>
        <dt class = "col-sm-10">
            @Html.DisplayNameFor(model => model.EmailUsuario)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.EmailUsuario)
        </dd>
        <dt class = "col-sm-10">
            @Html.DisplayNameFor(model => model.SenhaUsuario)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.SenhaUsuario)
        </dd>
        <dd class="col-sm-10">
            <a asp-action="Edit" asp-route-id="@Model?.Id" class="voltar">Editar</a> |
            <a asp-action="Index" class="voltar">Voltar</a>
        </dd>
    </dl>   
</div>
